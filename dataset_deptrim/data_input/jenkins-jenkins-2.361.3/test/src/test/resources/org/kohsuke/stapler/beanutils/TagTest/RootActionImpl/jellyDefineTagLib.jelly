<?jelly escape-by-default='true'?>
<j:jelly xmlns:j="jelly:core" xmlns:st="jelly:stapler" xmlns:d="jelly:define">
    <st:contentType value="text/html"/>

    <d:taglib uri="local">
        <d:tag name="item">
            <div class="${class}">
                Label:${label}
            </div>
        </d:tag>
    </d:taglib>

    <div xmlns:local="local">
        <local:item class="theFirstClass" label="theFirstLabel"/>
        <local:item class="theSecondClass" label="theSecondLabel"/>
    </div>
</j:jelly>
