<?jelly escape-by-default='true'?>
<j:jelly xmlns:j="jelly:core" xmlns:l="/lib/layout">
  <l:layout>
    <l:main-panel>
      <div id="symbol-icons">
        <div class="unsafe">
          <l:icon src="symbol-play" tooltip="${it.unsafeTooltip}" />
        </div>
        <div class="safe">
          <l:icon src="symbol-play" tooltip="${it.safeTooltip}" />
        </div>
      </div>

      <div id="svgIcons">
        <div class="unsafe">
          <l:svgIcon href="TODO" tooltip="${it.unsafeTooltip}" />
