          <l:svgIcon href="TODO" tooltip="${it.safeTooltip}" />
        </div>
      </div>

      <div id="png-icons">
        <div class="unsafe">
          <l:icon src="security2776-nonexistent.png" tooltip="${it.unsafeTooltip}" />
        </div>
        <div class="safe">
          <l:icon src="security2776-nonexistent.png" tooltip="${it.safeTooltip}" />
        </div>
      </div>
    </l:main-panel>
