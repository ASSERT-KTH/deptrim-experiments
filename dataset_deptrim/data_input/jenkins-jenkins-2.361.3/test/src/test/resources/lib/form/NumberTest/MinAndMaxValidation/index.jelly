<?jelly escape-by-default='true'?>
<j:jelly xmlns:j="jelly:core" xmlns:st="jelly:stapler" xmlns:d="jelly:define" xmlns:l="/lib/layout" xmlns:t="/lib/hudson" xmlns:f="/lib/form">
    <l:layout title="Min/Max Validation for f:number">
        <l:main-panel>
            <f:form name="number">
                <f:entry title="min-5, max-70">
                    <f:number clazz="number" name="min-5-max-70" min="5" max="70"/>
                </f:entry>
                <f:entry title="min-70, max-5">
                    <f:number clazz="number" name="min-70-max-5" min="70" max="5"/>
                </f:entry>
                <f:entry title="min-5, max-wow">
                    <f:number clazz="number" name="min-5-max-wow" min="5" max="wow"/>
                </f:entry>
                <f:entry title="min-wow, max-70">
                    <f:number clazz="number" name="min-wow-max-70" min="wow" max="70"/>
                </f:entry>
                <f:entry title="min-wow, max-jen">
                    <f:number clazz="number" name="min-wow-max-jen" min="wow" max="jen"/>
                </f:entry>

            </f:form>
        </l:main-panel>
    </l:layout>
</j:jelly>
